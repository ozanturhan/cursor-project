{
  "name": "consultation-platform",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "test:e2e": "turbo test:e2e",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prisma": "^6.1.0",
    "turbo": "^2.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0"
  }
}
